<%- include("partials/header") -%>

      </header>

      <main class="project">

        <div class="project-left-contaner">
            <div class="p-text-container">
                <h1>Title</h1>
                <p class="p-text">web dev project</p>
            </div>
    
            <div class="p-text-container mt-3 mb-3">
                <h1>description</h1>
                <p class="p-text">e-commerce website for selling clothes and accesories online</p>

            </div>
    
            <div class="p-text-container">
                <h1>Skills Used</h1>
                <ul class="p-text">
                    <li>HTML</li>
                    <li>Java Script</li>
                    <li>CSS</li>
                </ul>


            </div>
        </div>

        <div class="project-right-contaner">
            <div class="project-images">
                <!-- <img src="images/1.png" alt=""> -->
                        <!-- Carousel -->

                <div id="carousel" class="carousel carousel-project slide">
                    <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img src="images/laptop_12.jpg" class="d-block w-100" alt="...">
                    </div>
                    <div class="carousel-item">
                        <img src="images/2.webp" class="d-block w-100" alt="...">
                    </div>
                    <div class="carousel-item">
                        <img src="images/3.jpeg" class="d-block w-100" alt="...">
                    </div>
                    </div>
                    <button class="carousel-control-prev" type="button" data-bs-target="#carousel" data-bs-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Previous</span>
                    </button>
                    <button class="carousel-control-next" type="button" data-bs-target="#carousel" data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                    </button>
                </div>


            </div>
            
            <!-- <button class="btn btn-primary btn-lg mt-3" id="applicationBtn">Apply</button> -->
            <div class="p-text-container">
                <h1>completion date:</h1>
                <p class="p-text">31/03/2023</p>
            </div>  
        </div>
      </main>

      <hr class="divider">
      <div class="teammateCards">
        
        <div class="row">
            <div class="main-buttons">
                <button class="btn btn-primary btn-lg mt-3" id="add-teammate-btn">Add teammate</button>
            </div>
            <div class="applicationCard">
                <h3>Emad</h3>
                <p>helped developed the UI and front end functionality</p>
            </div>
        </div>
      </div>
      
      <div class="popupWindow hidePopupWindow" id="addTeammatePopupWindow">
        <div class="popup-message">
            <img class="mb-2" src="images/teammate.png">
            <h3>Add Teammate</h3>
        </div>
        <div class="popup-content">
            <form action="">
                <div class="form-group row  mt-3">
                    <label for="name" class="col-sm-4 col-form-label" >Name</label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control"  placeholder="Teammate's name" id="name" required>
                    </div>    
                </div>
                <div class="form-group row  mt-3">
                    <label for="task" class="col-sm-4 col-form-label" >task</label>
                    <div class="col-sm-8">
                        <textarea type="text" class="form-control"  placeholder="Teammate's tasks" id="task" required></textarea>
                    </div>    
                </div>
                <button class="btn btn-success btn-lg mt-3 mb-2" id="addTeammate">Add</button>
                
            </form>
            <a class="block" id="cancelAdding">Cancel</a>
        </div>
    </div>
<%- include("partials/footer") -%>